{
  "name": "telegram-faceswap-bot",
  "version": "1.0.0",
  "description": "Telegram bot for face swapping with payment integration",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "deploy": "./deploy.sh production",
    "deploy:staging": "./deploy.sh staging",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs telegram-faceswap-bot",
    "pm2:monit": "pm2 monit",
    "health:check": "curl http://localhost:3000/health",
    "backup:create": "node scripts/create-backup.js",
    "backup:restore": "node scripts/restore-backup.js",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix"
  },
  "keywords": [
    "telegram",
    "bot",
    "face-swap",
    "ai",
    "payment",
    "stripe",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-wasm": "^4.22.0",
    "@vladmandic/face-api": "^1.7.15",
    "axios": "^1.13.2",
    "cloudinary": "^1.41.1",
    "better-sqlite3": "^12.5.0",
    "canvas": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "stripe": "^20.0.0",
    "telegraf": "^4.16.3",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0",
    "supertest": "^6.3.3",
    "typescript": "^5.6.3",
    "@types/node": "^20.11.25"
  }
}
